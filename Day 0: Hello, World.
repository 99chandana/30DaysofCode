import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    
        Scanner myScanner = new Scanner(System.in);
        
        String inputString = myScanner.nextLine();
        
        System.out.println("Hello, World.\n" + inputString);
        
        myScanner.close();
        
    
        
    }
}
